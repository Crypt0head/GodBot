{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "GodBot",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceRoot}/src/main.cpp",
                "${workspaceRoot}/src/GodBot.cpp",
                "${workspaceRoot}/src/binance_api.cpp",
                "${workspaceRoot}/src/base_api.cpp",
                "${workspaceRoot}/src/Kline.cpp",
                "${workspaceRoot}/src/curl_object.cpp",
                "${workspaceRoot}/src/LogData.cpp",
                "-o",
                "${workspaceRoot}/bin/App.out",
                "-lcurl",
                "-lssl",
                "-lcrypto",
                "-lpthread",
                "-lboost_program_options",
                "-std=c++20"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "GodBot release",
            "command": "/usr/bin/g++",
            "args": [
                "-O2",
                "${workspaceRoot}/src/main.cpp",
                "-o",
                "${workspaceRoot}/bin/App.out",
                "-lcurl",
                "-lssl",
                "-lcrypto",
                "-lpthread",
                "-lboost_program_options"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "UNIT-TESTS",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceRoot}/tests/general_tests.cpp",
                "-o",
                "${workspaceRoot}/bin/Test.out",
                "-lcurl",
                "-lssl",
                "-lcrypto"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Unit-Tests"
        }
    ],
    "version": "2.0.0"
}